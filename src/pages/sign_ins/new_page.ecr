<div class="form-signin">
    <% form_for SignIns::Create do %>
        <h1 class="h3 mb-3 fw-normal text-start">Вход</h1>
    
        <% sign_in_fields(@operation) %>
        <% submit "Войти", class: "w-100 btn btn-lg btn-primary" %>
        <% link "Зарегистрироваться", to: SignUps::New %>
    <% end %>

</div>
